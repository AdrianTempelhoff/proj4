<h1>Listing temp_basket_items</h1>

<table>
  <tr>
    <th>Temp basket</th>
    <th>Item</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @temp_basket_items.each do |temp_basket_item| %>
  <tr>
    <td><%= temp_basket_item.temp_basket_id %></td>
    <td><%= temp_basket_item.item_id %></td>
    <td><%= link_to 'Show', temp_basket_item %></td>
    <td><%= link_to 'Edit', edit_temp_basket_item_path(temp_basket_item) %></td>
    <td><%= link_to 'Destroy', temp_basket_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

